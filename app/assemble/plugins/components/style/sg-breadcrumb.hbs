{{#styleguide "Breadcrumb" '
{{#prettify}}
{{#ul class="breadcrumb"}}
  {{#li}}{{a "Home" href="/index.html" class="link"}}{{/li}}
  {{#if navSection}}
    {{li navSection}}
  {{/if}}
  {{#if navSubSection}}
    {{li navSubSection}}
  {{/if}}
  {{li navTitle class="active"}}
{{/ul}}
{{/prettify}}
