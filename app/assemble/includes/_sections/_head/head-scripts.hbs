{{verbose "Scripts"}}
{{#devopts.modernizr}}
  {{#is js.modernizr}}
    {{js js.modernizr}}
  {{else}}
    {{js js.modernizr-dev}}
  {{/is}}
{{/devopts.modernizr}}
{{verbose "HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries"}}
{{verbose '[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]'}}
{{#is buildenv "production"}}
  {{verbose 'Google Analytics'}}
  {{codesnippet js.snippet-analytics}}
{{/is}}