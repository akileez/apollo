  {{ico img.apple-touch-icon-ipad3 'apple' 144}}
  {{ico img.apple-touch-icon-iphone4 'apple' 114}}
  {{ico img.apple-touch-icon-ipad 'apple' 72}}
  {{ico img.apple-touch-icon-iphone 'apple'}}
  {{ico img.favicon}}
{{include 'head-scripts'}}