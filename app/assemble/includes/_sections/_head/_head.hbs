{{verbose 'Meta'}}
  {{meta "charset"}}
  {{meta "viewport"}}
  {{meta "description" description}}
  {{meta "keywords" (default keywords site.keywords)}}
  {{meta "author" site.author}}
  {{#is buildenv "development"}}
    {{meta "revised"}}
  {{/is}}
  <title>{{default title site.title}}</title>
{{include 'head-social'}}
{{include 'head-styles'}}
