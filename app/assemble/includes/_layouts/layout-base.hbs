{{#prettify}}
<!DOCTYPE html>
<html lang="en">
<head>
  {{> _head }}
</head>
<body{{isScrollspy scrollspy}}>
  {{> _header }}
  {{#if stickyFooter}}<div id="wrap">{{/if}}
    <main{{isId idLayout}}{{isClass classLayout}}>
    {{#if containerLayout}}
      <div class="{{containerLayout}}">
      {{> body}}
      </div>
    {{else}}
      {{> body}}
    {{/if}}
    </main>
  {{#if stickFooter}}
    </div>
    {{div id="push"}}
  {{/if}}
  {{> _footer }}
</body>
</html>
{{/prettify}}